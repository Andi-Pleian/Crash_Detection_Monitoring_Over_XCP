void initCrashDetection() {
    CrashDetectionData.xVal = DEFAULT_AXIS_VALUE;
    CrashDetectionData.yVal = DEFAULT_AXIS_VALUE;
    CrashDetectionData.zVal = DEFAULT_AXIS_VALUE;

